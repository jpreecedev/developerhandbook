{
  "name": "developerhandbook",
  "author": "Jon Preece",
  "license": "MIT",
  "version": "3.11.0",
  "description": "Source code for DeveloperHandbook.com blog",
  "main": "index.js",
  "scripts": {
    "start": "gatsby develop",
    "prettier": "prettier \"./src/**/*.{js,json,yaml,yml,md}\" --write",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "bump": "bump --tag --push --all",
    "build": "rm -rf .cache public && gatsby build",
    "deploy": "npm run bump && npm run build && s3-deploy './public/**' --cwd './public/' --bucket developerhandbook.com  --deleteRemoved --gzip"
  },
  "homepage": "https://github.com/jpreecedev/developerhandbook#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpreecedev/developerhandbook.git"
  },
  "bugs": {
    "url": "https://github.com/jpreecedev/developerhandbook/issues"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "core-js": "^2.6.9",
    "disqus-react": "^1.0.7",
    "gatsby": "^2.15.20",
    "gatsby-image": "^2.2.20",
    "gatsby-plugin-feed": "^2.3.13",
    "gatsby-plugin-google-analytics": "^2.1.17",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-offline": "^3.0.8",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-postcss": "^2.1.8",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-remark-autolink-headers": "^2.1.11",
    "gatsby-remark-copy-linked-files": "^2.1.20",
    "gatsby-remark-images": "^3.1.23",
    "gatsby-remark-prismjs": "^3.3.14",
    "gatsby-remark-responsive-iframe": "^2.2.17",
    "gatsby-remark-smartypants": "^2.1.9",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-transformer-remark": "^2.6.24",
    "gatsby-transformer-sharp": "^2.2.16",
    "node-sass": "^4.12.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-reporter": "^6.0.1",
    "prismjs": "^1.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-lazy-load": "^3.0.13",
    "rehype-react": "^3.1.0"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gatsby-plugin-canonical-urls": "^2.1.8",
    "gatsby-plugin-no-sourcemaps": "^2.1.1",
    "gatsby-plugin-sass": "^2.1.15",
    "gatsby-plugin-sitemap": "^2.2.14",
    "prettier": "^1.18.2",
    "s3-deploy": "^1.4.0",
    "stylelint": "^11.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "version-bump-prompt": "^5.0.5"
  },
  "keywords": [
    "blog",
    "development",
    "c#",
    ".net",
    "code",
    "javascript",
    "react"
  ],
  "browserslist": [
    "defaults",
    "IE >= 11",
    "firefox >= 43"
  ]
}
